{
	"name": "@beyond-js/ai-tools",
	"version": "0.0.1",
	"description": "BeyondJS AI Server",
	"repository": {
		"type": "git",
		"url": "https://github.com/beyondjs/ai-tools"
	},
	"static": {
		"includes": [
			"openapi"
		]
	},
	"modules": {
		"path": "modules"
	},
	"dependencies": {
		"@beyond-js/kernel": "~0.1.9",
		"dotenv": "~16.3.1"
	},
	"devDependencies": {
		"@beyond-js/local": "~0.1.3",
		"@beyond-js/bee": "^0.0.6",
		"@types/express": "^4.17.17",
		"@types/node": "~20.14.5"
	},
	"deployment": {
		"distributions": [
			{
				"name": "node",
				"platform": "node",
				"ports": {
					"bundles": "3000"
				}
			},
			{
				"name": "node-ts",
				"platform": "node",
				"ts": {
					"compiler": "tsc"
				},
				"ports": {
					"bundles": "3001"
				}
			}
		]
	}
}
